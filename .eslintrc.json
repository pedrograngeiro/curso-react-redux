{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "airbnb", "prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/prop-types": 0,
		"react/destructuring-assignment": ["off"],
		"import/no-anonymous-default-export": [
			"error",
			{
				"allowArrowFunction": true
			}
		],
		"react/jsx-props-no-spreading": "off"
	}
}
